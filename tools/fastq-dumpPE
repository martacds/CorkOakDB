   PATH=$PATH:~/Documents/tools/fastq-dump/bin/
   export PATH

counter=0
for SRA_ID in `cat $1`
do
   counter=$((counter+1))
   PROJECTNAME=`echo $SRA_ID | cut -c1-6`
   
   echo ' '
   echo '~~~~~~~~~~~~' $counter ' FASTQ-DUMP ' $SRA_ID '~~~~~~~~~~~~'
   fastq-dump -I --split-files -O /data/biodata/fastqfiles/$SRA_ID $SRA_ID
done

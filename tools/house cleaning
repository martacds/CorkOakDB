input="$DATA/listfilenames454.txt"
while IFS= read -r FILENAME
do
   PROJECTNAME=`echo $FILENAME | cut -c1-6`
   cd $BIO

   mkdir -p notag
   mkdir -p sickle
   mkdir -p prinseq
   mkdir -p gmap
   mkdir -p bam
   mkdir -p featurecounts
   mkdir -p stringtie

   mv sickle* ./sickle
   mv notag* ./notag
   mv *.bam ./bam
   mv *.sam ./gmap
   mv abundance* ./stringtie
   mv *.gtf ./stringtie
   mv counts* ./featurecounts
   mv ps* ./prinseq

done < "$input"
